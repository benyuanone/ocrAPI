SELECT * FROM ourway_sys_dic,ourway_sys_dic_value WHERE ourway_sys_dic.OWID=ourway_sys_dic_value.DIC_REF_OWID AND ourway_sys_dic.TYPE=2;
DELETE FROM ourway_sys_dic_value WHERE DIC_REF_OWID IN (SELECT owid FROM ourway_sys_dic WHERE TYPE=2);
delete from `ourway_sys_dic` where type=2;


