   ADD COLUMN `DEPT_ID` INT NULL AFTER `STATE`,
   ADD COLUMN `DEPT_PATH` VARCHAR(240) NULL AFTER `DEPT_ID`;

ALTER TABLE `ourway_sys_form`
   ADD COLUMN `DEPT_ID` INT NULL AFTER `STATE`,
   ADD COLUMN `DEPT_PATH` VARCHAR(240) NULL AFTER `DEPT_ID`;

ALTER TABLE `ourway_sys_historyform`
   ADD COLUMN `DEPT_ID` INT NULL AFTER `STATE`,
   ADD COLUMN `DEPT_PATH` VARCHAR(240) NULL AFTER `DEPT_ID`;

ALTER TABLE `ourway_sys_tempcontrol`
   ADD COLUMN `DEPT_ID` INT NULL,
   ADD COLUMN `DEPT_PATH` VARCHAR(240) NULL;

ALTER TABLE `ourway_sys_workflow`
   ADD COLUMN `DEPT_ID` INT NULL,
   ADD COLUMN `DEPT_PATH` VARCHAR(240) NULL;

