alter table `ourway_sys_flownode_per`
   add column `PER_FILTER_TYPE` tinyint NULL ;

alter table ourway_sys_employs
   add column `EMP_DEFAULT_DEPT` int NULL,
   add column `EMP_DEFAULT_POSITION` varchar(64) NULL;

alter table ourway_sys_depatemp
   add column `EMP_DEFAULT` tinyint NULL ;

alter table `owboss`.`ourway_sys_depat`
   add column `DEPT_ID` int NULL ,
   add column `DEPT_PATH` varchar(240) NULL ;

